<h1>Editing Book</h1>
<!-- form_for部分 -->
<%= form_for(@book) do |f| %>
    <h4>タイトル</h4>
    <%= f.text_field :title %>
    <h4>本文</h4>
    <%= f.text_area :body %>
    <br>
    <%= f.submit 'update book' %>
    <br>
    <%= link_to "edit", edit_book_path %>
	<%= link_to "back", books_path %>
<% end %>